<div class="span12">
	{{#if content_data}}
	<table class="table table-striped table-bordered table-hover table-condensed data-table">
		<thead>
			<tr>
				<th class="center">#</th>
				<th>Supply Name</th>
				<th class="hidden-480">Description</th>
				<th>Category</th>
				<th class="hidden-480">Re-order Level</th>
				<th class="hidden-480">Target Level</th>
				<th>Price per Unit</th>
			</tr>
		</thead>
		<tbody>
			{{#iter content_data}}
			<tr>
				<td class="center">{{@index}}</td>
				<td>{{supply_name}}</td>
				<td>{{supply_description}}</td>
				<td class="hidden-480">{{supply_unit_type.supply_type_name}}</td>
				<td class="hidden-480">{{reorder_level}}</td>
				<td>{{target_level}}</td>
				<td class="hidden-480">{{price_per_unit}}</td>
			</tr>
			{{/iter}}
		</tbody>
	</table>
	{{else}}
	<p>No items found.</p>
	{{/if}}
</div>